{
  "Gelbooru": {
    "url": "https://gelbooru.com/index.php?page=post&s=list",
    "thumbnailSelector": "article.thumbnail-preview",
    "imgSelector": ".image-container picture img",
    "pageSelector": "#paginator a",
    "pageEndpoint": "&pid=",
    "pageMultiplier": 24
  },
  "Danbooru": {
    "url": "https://danbooru.donmai.us/posts?",
    "thumbnailSelector": ".post-preview-container",
    "imgSelector": "picture img",
    "pageSelector": ".paginator a",
    "pageEndpoint": "&page=",
    "pageMultiplier": 1
  },
  "Safebooru": {
    "url": "https://safebooru.org/index.php?page=post&s=list",
    "thumbnailSelector": ".thumb",
    "imgSelector": ".content img#image",
    "pageSelector": ".pagination a",
    "pageEndpoint": "&pid=",
    "pageMultiplier": 20
  }
}
